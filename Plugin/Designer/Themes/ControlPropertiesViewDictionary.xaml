<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:designerItems="clr-namespace:Designer.DesignerItems">
    <DataTemplate DataType="{x:Type designerItems:BlockProperties}">
        <designerItems:BlockPropertiesView/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type designerItems:TableProperties}">
        <designerItems:TablePropertiesView/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type designerItems:RowSet}">
        <designerItems:RowSetPropertiesView/>
    </DataTemplate>
</ResourceDictionary>